            $('.slider_main').on('change.owl.carousel', function (event) {
        var el = event.target;
        $('h1, p, .caption_button, button', el).addClass('flipInX animated')
                .one('webkitAnimationEnd mozAnimationEnd MSAnimationEnd oanimationend animationend', function () {
                    $('h1, p, .caption_button, button', el).removeClass('flipInX animated');
