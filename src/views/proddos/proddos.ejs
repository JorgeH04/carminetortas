                    <!-- FIRST ITEM -->
                    <% if(current == 1) { %>
                      <li class="page-item disabled">
                        <a class="page-link" href="#">First</a>
                      </li>
                    <% } else { %>
                      <li class="page-item">
                        <a class="page-link" href="/proddosindex/1">First</a>
                      </li>
                    <% } %>
                    <!-- ITEMS  -->
                    <% var i = (Number(current) > 5 ? Number(current) - 4 : 1) %>
                    <% if(i !== 1) { %>
                      <li class="page-item disabled">
                        <a class="page-link" href="#">...</a>
                      </li>
                    <% } %>
                    <% for(; i <= (Number(current) + 4) && i <= pages; i++) { %>
                      <% if(i == current) { %>
                        <li class="page-item active">
                          <a class="page-link" href="<%= i %>">
                            <%= i %>
                          </a>
                        </li>
                      <% } else { %>
                        <li class="page-item">
                          <a class="page-link" href="/proddosindex/<%= i %>">
                            <%= i %>
                          </a>
                        </li>
                      <% } %>
                      <% if (i == Number(current) + 4 && i < pages) { %>
                         <li class="page-item disabled">
                           <a class="page-link" href="#">...</a>
                         </li>
                      <% } %>
                    <% } %>
                    <!-- LAST ITEM -->
                    <% if(current == pages) { %>
                      <li class="page-item disabled">
                        <a class="page-link" href="#">
                          Last
                        </a>
                      </li>
                    <% } else { %>
                      <li class="page-item">
                        <a class="page-link" href="/proddosindex/<%= pages %>">
                          Last
                        </a>
                      </li>
                    <% } %>
                   
              <% } %>
              



